{"ast":null,"code":"import { mapState, mapGetters, mapActions } from 'vuex';\nexport default {\n  name: 'Leave',\n  data() {\n    return {\n      leaveType: '',\n      startDate: '',\n      endDate: '',\n      reason: ''\n    };\n  },\n  computed: {\n    ...mapState(['annualLeaveBalance']),\n    ...mapGetters(['leaveApplications'])\n  },\n  methods: {\n    ...mapActions(['submitLeaveApplication']),\n    submitLeave() {\n      const start = new Date(this.startDate);\n      const end = new Date(this.endDate);\n      const days = Math.floor((end - start) / (1000 * 60 * 60 * 24)) + 1;\n      if (days > this.annualLeaveBalance) {\n        alert('Jumlah hari cuti melebihi sisa cuti tahunan.');\n        return;\n      }\n      const application = {\n        leaveType: this.leaveType,\n        startDate: this.startDate,\n        endDate: this.endDate,\n        days,\n        reason: this.reason\n      };\n      this.submitLeaveApplication(application);\n\n      // Clear form\n      this.leaveType = '';\n      this.startDate = '';\n      this.endDate = '';\n      this.reason = '';\n    }\n  }\n};","map":{"version":3,"names":["mapState","mapGetters","mapActions","name","data","leaveType","startDate","endDate","reason","computed","methods","submitLeave","start","Date","end","days","Math","floor","annualLeaveBalance","alert","application","submitLeaveApplication"],"sources":["E:\\GitHub\\11-1-pencatatan-absen-lembur-gaji-cuti\\src\\components\\Leave.vue"],"sourcesContent":["<template>\r\n  <div class=\"container mt-5\">\r\n    <div class=\"row\">\r\n      <div class=\"col-lg-6 col-md-12 mb-3\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <h5 class=\"card-title\">Formulir Pengajuan Cuti</h5>\r\n            <form @submit.prevent=\"submitLeave\">\r\n              <div class=\"mb-3\">\r\n                <label for=\"leaveType\" class=\"form-label\">Jenis Cuti</label>\r\n                <select id=\"leaveType\" class=\"form-select\" v-model=\"leaveType\" required>\r\n                  <option value=\"Cuti Tahunan\">Cuti Tahunan</option>\r\n                  <option value=\"Cuti Sakit\">Cuti Sakit</option>\r\n                  <option value=\"Cuti Melahirkan\">Cuti Melahirkan</option>\r\n                  <!-- Tambahkan jenis cuti lainnya di sini -->\r\n                </select>\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label for=\"startDate\" class=\"form-label\">Tanggal Mulai</label>\r\n                <input type=\"date\" id=\"startDate\" class=\"form-control\" v-model=\"startDate\" required>\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label for=\"endDate\" class=\"form-label\">Tanggal Selesai</label>\r\n                <input type=\"date\" id=\"endDate\" class=\"form-control\" v-model=\"endDate\" required>\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label for=\"reason\" class=\"form-label\">Alasan</label>\r\n                <textarea id=\"reason\" class=\"form-control\" v-model=\"reason\" rows=\"3\" required></textarea>\r\n              </div>\r\n              <button type=\"submit\" class=\"btn btn-primary\">Ajukan Cuti</button>\r\n            </form>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"col-lg-6 col-md-12 mb-3\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <h5 class=\"card-title\">Sisa Cuti Tahunan</h5>\r\n            <p class=\"card-text\">{{ annualLeaveBalance }} hari</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"row\">\r\n      <div class=\"col-lg-12 col-md-12 mb-3\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <h5 class=\"card-title\">Riwayat Pengajuan Cuti</h5>\r\n            <table class=\"table\">\r\n              <thead>\r\n                <tr>\r\n                  <th>Jenis Cuti</th>\r\n                  <th>Tanggal Mulai</th>\r\n                  <th>Tanggal Selesai</th>\r\n                  <th>Jumlah Hari</th>\r\n                  <th>Alasan</th>\r\n                </tr>\r\n              </thead>\r\n              <tbody>\r\n                <tr v-for=\"(application, index) in leaveApplications\" :key=\"index\">\r\n                  <td>{{ application.leaveType }}</td>\r\n                  <td>{{ application.startDate }}</td>\r\n                  <td>{{ application.endDate }}</td>\r\n                  <td>{{ application.days }}</td>\r\n                  <td>{{ application.reason }}</td>\r\n                </tr>\r\n              </tbody>\r\n            </table>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState, mapGetters, mapActions } from 'vuex';\r\n\r\nexport default {\r\n  name: 'Leave',\r\n  data() {\r\n    return {\r\n      leaveType: '',\r\n      startDate: '',\r\n      endDate: '',\r\n      reason: ''\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState(['annualLeaveBalance']),\r\n    ...mapGetters(['leaveApplications'])\r\n  },\r\n  methods: {\r\n    ...mapActions(['submitLeaveApplication']),\r\n    submitLeave() {\r\n      const start = new Date(this.startDate);\r\n      const end = new Date(this.endDate);\r\n      const days = Math.floor((end - start) / (1000 * 60 * 60 * 24)) + 1;\r\n\r\n      if (days > this.annualLeaveBalance) {\r\n        alert('Jumlah hari cuti melebihi sisa cuti tahunan.');\r\n        return;\r\n      }\r\n\r\n      const application = {\r\n        leaveType: this.leaveType,\r\n        startDate: this.startDate,\r\n        endDate: this.endDate,\r\n        days,\r\n        reason: this.reason\r\n      };\r\n\r\n      this.submitLeaveApplication(application);\r\n\r\n      // Clear form\r\n      this.leaveType = '';\r\n      this.startDate = '';\r\n      this.endDate = '';\r\n      this.reason = '';\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\