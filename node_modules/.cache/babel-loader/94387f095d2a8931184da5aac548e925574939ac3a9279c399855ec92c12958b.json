\n\r\n.kehadiran-table {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n  background-color: #ffffff; /* Background color putih */\r\n}\r\n\r\n\r\n.kehadiran-table th,\r\n.kehadiran-table td {\r\n  border: 1px solid #ddd;\r\n  padding: 8px;\r\n}\r\n\r\n.kehadiran-table th {\r\n  background-color: #f2f2f2;\r\n}\r\n</style>\r\n"],"mappings":"AAgCA,eAAe;EACbA,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,aAAa,EAAE,CACb;QAAEC,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,UAAU;QAAEC,OAAO,EAAE,YAAY;QAAEC,MAAM,EAAE,OAAO;QAAEC,QAAQ,EAAE,OAAO;QAAEC,SAAS,EAAE,OAAO;QAAEC,SAAS,EAAE;MAAK,CAAC,EAC3H;QAAEN,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,YAAY;QAAEC,OAAO,EAAE,YAAY;QAAEC,MAAM,EAAE,OAAO;QAAEC,QAAQ,EAAE,OAAO;QAAEC,SAAS,EAAE,OAAO;QAAEC,SAAS,EAAE;MAAM;MAC7H;MAAA;IAEJ,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,iBAAiBA,CAAA,EAAG;MAClB,OAAO,IAAI,CAACT,aAAa,CAACU,MAAM,CAACC,SAAQ,IACvCA,SAAS,CAACT,IAAI,CAACU,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACd,WAAW,CAACa,WAAW,CAAC,CAAC,KACpED,SAAS,CAACR,OAAO,CAACU,QAAQ,CAAC,IAAI,CAACd,WAAW,CAACa,WAAW,CAAC,CAAC,KACzDD,SAAS,CAACP,MAAM,CAACQ,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACd,WAAW,CAACa,WAAW,CAAC,CAAC,KACtED,SAAS,CAACN,QAAQ,CAACQ,QAAQ,CAAC,IAAI,CAACd,WAAW,CAACa,WAAW,CAAC,CAAC,KAC1DD,SAAS,CAACL,SAAS,CAACO,QAAQ,CAAC,IAAI,CAACd,WAAW,CAACa,WAAW,CAAC,CAAC,KAC3DD,SAAS,CAACJ,SAAS,CAACK,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACd,WAAW,CAACa,WAAW,CAAC,CAAC,CAC3E,CAAC;IACH;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}